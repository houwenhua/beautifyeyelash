<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    
<hibernate-mapping>
	<class name="com.meijie.entity.Tree" table="t_nav">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="identity"></generator>
		</id>
		<property name="text" column="text" length="20" not-null="true"></property>
		<property name="state" column="state" length="10" not-null="true"></property>
		<property name="iconCls" column="iconCls" length="20" not-null="true"></property>
		<property name="url" column="url" length="50" not-null="true"></property>
		<property name="tid" column="tid" not-null="true"></property>
	</class>
</hibernate-mapping>